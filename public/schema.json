{
    "nodes": [
        {
            "node": "Transcription Factor",
            "example": [
                "CHCHD3",
                "PITX3"
            ],
            "display": [
                {
                    "label": "Gene ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "Gene symbol",
                    "text": "${label}",
                    "type": "text"
                },
                {
                    "label": "URI",
                    "text": "${uri}",
                    "href": "${uri}",
                    "type": "link"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ],
            "color": "#80D8FF"
        }
    ],
    "edges": [
        {
            "match": [
                "upregulates"
            ],
            "color": "#0ac749",
            "directed": "triangle",
            "display": [
                {
                    "label": "source",
                    "text": "${source}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target",
                    "text": "${target}",
                    "type": "text"
                },
                {
                    "label": "z score",
                    "text": "${z_score}",
                    "type": "text"
                },
                {
                    "label": "p value",
                    "text": "${p_value}",
                    "type": "text"
                }
            ]
        },
        {
            "match": [
                "downregulates"
            ],
            "color": "#c70a2d",
            "directed": "triangle",
            "display": [
                {
                    "label": "source",
                    "text": "${source}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target",
                    "text": "${target}",
                    "type": "text"
                },
                {
                    "label": "z score",
                    "text": "${z_score}",
                    "type": "text"
                },
                {
                    "label": "p value",
                    "text": "${p_value}",
                    "type": "text"
                }
            ]
        }
    ],
    "header": {
        "divider": false,
        "header": "hGRN ChEAR",
        "title": "hGRN ChEAR",
        "background": {
            "backgroundColor": "#000",
            "contrastText": "#FFF"
        },
        "icon": {
            "src": "/tf2tf_logo.png",
            "favicon": "/tf2tf_logo.png",
            "faviconTitle": "hGRN_ChEAR_icon",
            "alt": "hGRN ChEAR",
            "key": "hGRN_ChEAR_icon",
            "width": 200,
            "height": 120,
            "avatar": false
        },
        "tabs": [
            {
                "endpoint": "/",
                "label": "Transcription Factor Search",
                "type": "page",
                "component": "KnowledgeGraph",
                "props": {
                    "init_function": "initialize_kg",
                    "initial_query": {
                        "start": "Transcription Factor",
                        "start_term": "LYL1",
                        "start_field": "label"
                    },
                    "subheader": {
                        "url_field": "filter",
                        "query_field": "relation"
                    }
                }
            }
        ]
    },
    "footer": {
        "style": {
            "background": "#1F3D5C",
            "color": "#FFF"
        },
        "layout": [
            [
                {
                    "component": "logo",
                    "props": {
                        "src": "/tf2tf_logo.png",
                        "alt": "hGRN_ChEAR_logo",
                        "title": "hGRN_ChEAR",
